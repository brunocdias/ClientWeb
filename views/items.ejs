<% include partials/header %>

<h1>Lista de items</h1>

<ul>
    <% data.forEach(function(item) { %>
      <li>
      	  <%= item["id"]%> - 	
          <strong>
          <%= item["name"] %>
          </strong>
          - <%=item["category"]%>
          - <%=item["price"]%>
      </li> 
    <% }) %>
</ul>

<% include partials/footer %>